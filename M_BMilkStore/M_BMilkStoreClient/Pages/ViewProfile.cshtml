@page
@model M_BMilkStoreClient.Pages.ViewProfileModel
@{
    ViewData["Title"] = "View Profile";
}
<h2>@ViewData["Title"]</h2>
<div>
    
    
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            LastName
        </dt>
        <dd class="col-sm-10">
            @Model.User.LastName
        </dd>

        <dt class="col-sm-2">
            FirstName
        </dt>
        <dd class="col-sm-10">
            @Model.User.FirstName
        </dd>
        <dt class="col-sm-2">
            Address
        </dt>
        <dd class="col-sm-10">
            @Model.User.Address
        </dd>
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Model.User.Name
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.User.Email
        </dd>
        <dt class="col-sm-2">
            Password
        </dt>
        <dd class="col-sm-10">
           @Model.User.Password
        </dd>
        <dt class="col-sm-2">
            PhoneNumber
        </dt>
        <dd class="col-sm-10">
            @Model.User.PhoneNumber
        </dd>
    </dl>
</div>

<form method="post">
    
    <a asp-page="/EditProfile" class="btn btn-secondary">EditProfile</a>
    <a asp-page="/Index" class="btn btn-secondary">Cancel</a>
</form>
